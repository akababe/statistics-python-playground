    "# üéÆ PLAYGROUND ZONE - Ubah parameter!\n",
    "# ==========================================\n",
    "\n",
    "num_rolls = 6000         # Coba: 600, 1200, 12000\n",
    "num_dice = 1             # Coba: 1, 2 (untuk 2 dadu)\n",
    "target_number = 6        # Event yang kita cari (1-6 untuk 1 dadu)\n",
    "\n",
    "# ==========================================\n",
    "\n",
    "np.random.seed(42)\n",
    "\n",
    "# Sample Space untuk 1 dadu\n",
    "sample_space_dice = [1, 2, 3, 4, 5, 6]\n",
    "\n",
    "# Simulate dice rolls\n",
    "if num_dice == 1:\n",
    "    rolls = np.random.choice(sample_space_dice, size=num_rolls)\n",
    "    possible_outcomes = sample_space_dice\n",
    "else:  # 2 dice\n",
    "    rolls = np.random.choice(sample_space_dice, size=(num_rolls, num_dice))\n",
    "    rolls = rolls.sum(axis=1)  # Sum of two dice\n",
    "    possible_outcomes = range(2, 13)  # Sum bisa dari 2 sampai 12\n",
    "\n",
    "# Count results\n",
    "unique, counts = np.unique(rolls, return_counts=True)\n",
    "results_dict = dict(zip(unique, counts))\n",
    "\n",
    "# Theoretical probability\n",
    "if num_dice == 1:\n",
    "    theoretical_prob = 1/6\n",
    "    total_outcomes = 6\n",
    "else:\n",
    "    # For 2 dice, different sums have different probabilities\n",
    "    theoretical_prob = {2: 1/36, 3: 2/36, 4: 3/36, 5: 4/36, 6: 5/36,\n",
    "                       7: 6/36, 8: 5/36, 9: 4/36, 10: 3/36, 11: 2/36, 12: 1/36}\n",
    "    total_outcomes = 36\n",
    "\n",
    "print(\"=\"*60)\n",
    "print(f\"üé≤ DICE ROLL SIMULATION ({num_dice} {'DIE' if num_dice == 1 else 'DICE'})\")\n",
    "print(\"=\"*60)\n",
    "print(f\"\\n‚öôÔ∏è  Settings:\")\n",
    "print(f\"   Number of rolls: {num_rolls:,}\")\n",
    "print(f\"   Number of dice: {num_dice}\")\n",
    "print(f\"   Sample space size: {total_outcomes}\")\n",
    "\n",
    "print(f\"\\nüìä Results for Each Outcome:\")\n",
    "print(f\"   {'Outcome':<10} {'Count':<10} {'Observed P':<15} {'Theoretical P':<15} {'Diff'}\")\n",
    "print(f\"   {'-'*60}\")\n",
    "\n",
    "for outcome in possible_outcomes:\n",
    "    count = results_dict.get(outcome, 0)\n",
