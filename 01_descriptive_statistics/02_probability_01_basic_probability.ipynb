    "    if num_dice == 1:\n",
    "        theo_prob = theoretical_prob\n",
    "    else:\n",
    "        theo_prob = theoretical_prob.get(outcome, 0)\n",
    "    \n",
    "    diff = abs(obs_prob - theo_prob)\n",
    "    print(f\"   {outcome:<10} {count:<10} {obs_prob:<15.4f} {theo_prob:<15.4f} {diff:.4f}\")\n",
    "\n",
    "# Specific event probability\n",
    "if num_dice == 1 and target_number in sample_space_dice:\n",
    "    count_target = results_dict.get(target_number, 0)\n",
    "    obs_prob_target = count_target / num_rolls\n",
    "    \n",
    "    print(f\"\\nüéØ Specific Event: Getting {target_number}\")\n",
    "    print(f\"   Observed P({target_number}) = {count_target}/{num_rolls} = {obs_prob_target:.4f}\")\n",
    "    print(f\"   Theoretical P({target_number}) = 1/6 = {1/6:.4f}\")\n",
    "    print(f\"   Difference: {abs(obs_prob_target - 1/6):.4f}\")\n",
    "\n",
    "print(f\"\\nüí° Probability Concepts:\")\n",
    "print(f\"   1. Each outcome is equally likely (for fair die): P = 1/{total_outcomes}\")\n",
    "print(f\"   2. All probabilities sum to 1: Œ£P = 1.0000\")\n",
    "print(f\"   3. Complement: P(not {target_number}) = 1 - P({target_number}) = {1 - 1/6:.4f}\")\n",
    "print(\"=\"*60)"
   ]
