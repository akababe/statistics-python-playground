   "source": [
    "# üéÆ PLAYGROUND ZONE - Ubah parameter!\n",
    "# ==========================================\n",
    "\n",
    "num_flips = 1000      # Coba: 100, 500, 5000, 10000\n",
    "fair_coin = True      # Coba: True (fair), False (biased)\n",
    "bias_prob = 0.7       # Jika biased, probabilitas Heads (0.5 = fair)\n",
    "\n",
    "# ==========================================\n",
    "\n",
    "np.random.seed(42)\n",
    "\n",
    "# Sample Space\n",
    "sample_space = ['H', 'T']  # Heads, Tails\n",
    "\n",
    "# Probability\n",
    "if fair_coin:\n",
    "    p_heads = 0.5\n",
    "    p_tails = 0.5\n",
    "else:\n",
    "    p_heads = bias_prob\n",
    "    p_tails = 1 - bias_prob\n",
    "\n",
    "# Simulate coin flips\n",
    "flips = np.random.choice(sample_space, size=num_flips, p=[p_heads, p_tails])\n",
    "\n",
    "# Count results\n",
    "count_heads = np.sum(flips == 'H')\n",
    "count_tails = np.sum(flips == 'T')\n",
    "\n",
    "# Observed probability\n",
    "observed_p_heads = count_heads / num_flips\n",
    "observed_p_tails = count_tails / num_flips\n",
    "\n",
    "print(\"=\"*60)\n",
    "print(\"ü™ô COIN FLIP SIMULATION\")\n",
    "print(\"=\"*60)\n",
    "print(f\"\\n‚öôÔ∏è  Settings:\")\n",
    "print(f\"   Number of flips: {num_flips:,}\")\n",
    "print(f\"   Coin type: {'FAIR (50-50)' if fair_coin else f'BIASED (Heads: {bias_prob:.0%})'}\")\n",
    "\n",
    "print(f\"\\nüìä Results:\")\n",
    "print(f\"   Heads (H): {count_heads:,} times ({observed_p_heads:.4f})\")\n",
    "print(f\"   Tails (T): {count_tails:,} times ({observed_p_tails:.4f})\")\n",
    "\n",
    "print(f\"\\nüéØ Theoretical Probability:\")\n",
    "print(f\"   P(H) = {p_heads:.4f}\")\n",
    "print(f\"   P(T) = {p_tails:.4f}\")\n",
    "\n",
    "print(f\"\\nüìà Observed vs Theoretical:\")\n",
    "print(f\"   Heads: Observed {observed_p_heads:.4f} vs Theoretical {p_heads:.4f}\")\n",
    "print(f\"   Difference: {abs(observed_p_heads - p_heads):.4f}\")\n",
    "\n",
    "print(f\"\\nüí° Insight:\")\n",
    "if abs(observed_p_heads - p_heads) < 0.02:\n",
    "    print(\"   ‚úÖ Hasil sangat mendekati teori (Law of Large Numbers bekerja!)\")\n",
    "elif abs(observed_p_heads - p_heads) < 0.05:\n",
    "    print(\"   ‚ö†Ô∏è  Hasil cukup mendekati teori (tambah jumlah flip untuk lebih akurat)\")\n",
    "else:\n",
    "    print(\"   ‚ùå Hasil jauh dari teori (mungkin perlu lebih banyak flip)\")\n",
    "\n",
    "print(\"\\nüìö Konsep: Complement Rule\")\n",
    "print(f\"   P(H) + P(T) = {observed_p_heads:.4f} + {observed_p_tails:.4f} = {observed_p_heads + observed_p_tails:.4f}\")\n",
    "print(f\"   P(T) = 1 - P(H) = 1 - {observed_p_heads:.4f} = {1 - observed_p_heads:.4f}\")\n",
    "print(\"=\"*60)"
   ]
